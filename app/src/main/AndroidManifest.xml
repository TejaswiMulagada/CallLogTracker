<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.call"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/cicon" android:label="@string/app_name">
        <activity android:name=".MainScreen"
                  android:label="@string/app_name" android:theme="@drawable/icon">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity android:name=".tabactivity"
                  android:label="@string/app_name" android:theme="@drawable/icon">
            <intent-filter>
                <action android:name="com.call.tabactivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            </activity>
        
        
<activity android:name=".IncomingCallActivity"/>
<activity android:name=".MissedCallActivity"/>
<activity android:name=".OutGoingCallActivity"/>
<activity android:name=".calllogs"/>
<activity android:name=".listofcalls"/>
<activity android:name=".chart"/>
<activity android:name=".Call"/>
<activity android:name=".MessageSending"/>
    </application>
<uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>
<uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
<uses-permission android:name="android.permission.RECEIVE_SMS"></uses-permission>
<uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>
</manifest> 